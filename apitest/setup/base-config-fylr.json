{
    "plugin": {
        "fylr-sequence-plugin": {
            "config": {
                "fylr-sequence-plugin.insert_sequence.objecttypes": {
                    "objecttype_settings": [
                        {
                            "update_objecttype": "cs_image",
                            "update_column": "cs_image.name",
                            "template": "HU%06d",
                            "only_insert": false
                        },
                        {
                            "update_objecttype": "cs_image",
                            "update_column": "cs_image.description",
                            "template": "DESC: #%04d",
                            "start_offset": 3000,
                            "only_insert": true
                        },
                        {
                            "update_objecttype": "inventory",
                            "update_column": "inventory.inv_nr",
                            "template": "INV_%05d",
                            "start_offset": 1000,
                            "only_insert": true
                        }
                    ]
                },
                "fylr-sequence-plugin.insert_sequence.sequence": {
                    "sequence_objecttype": "sequence",
                    "sequence_ref_field": "sequence.reference",
                    "sequence_num_field": "sequence.number"
                }
            }
        }
    }
}