[
    {
        "name": "enable fylr-plugin-sequence plugin",
        "request": {
            "endpoint": "plugin/manage/{{ datastore `plugin_id` }}",
            "method": "POST",
            "body": {
                "id": {{ datastore `plugin_id` }},
                "enabled": true
            }
        },
        "response": {
            "body": {
                "enabled": true
            }
        }
    }
]