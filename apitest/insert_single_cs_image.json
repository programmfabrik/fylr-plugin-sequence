[
    {{ range $idx, $v := N ( datastore `n_single_objects` ) }}
        {
            "name": "POST /api/v1/db/cs_image (#{{ $idx }})",
            "request": {
                "body": [
                    {
                        "_mask": "cs_image__all_fields",
                        "_objecttype": "cs_image",
                        "cs_image": {
                            "_version": 1,
                            "name": null
                        }
                    }
                ],
                "endpoint": "db/cs_image",
                "method": "POST",
                "query_params": {
                    "format": "long"
                }
            },
            "response": {
                {{ if (datastore `expect_error`) }}
                    "statuscode": 400
                {{ else }}
                    "body": [
                        {
                            "cs_image": {
                                "name:control": {
                                    "starts_with": "HU0",
                                    "ends_with": "0{{ add $idx 1 }}"
                                },
                                "description:control": {
                                    "starts_with": "DESC: #30",
                                    "ends_with": "{{ add $idx 1 }}"
                                }
                            }
                        }
                    ],
                    "statuscode": 200
                {{ end }}
            },
            "store_response_gjson": {
                "cs_image_id_{{ $idx }}": "body.0.cs_image._id",
                "cs_image_version_{{ $idx }}": "body.0.cs_image._version"
            }
        },
    {{ end }}
]